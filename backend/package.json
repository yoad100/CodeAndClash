    "lint": "eslint 'src/**/*.ts' --fix"
  },
  "dependencies": {
    "@socket.io/redis-adapter": "^8.1.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.0",
    "helmet": "^6.0.1",
    "http-errors": "^2.0.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.3.1",
    "morgan": "^1.10.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.8.1",
    "uuid": "^9.0.1",
    "winston": "^3.9.0",
