    "@types/morgan": "^1.9.10",
    "@types/node": "^20.4.2",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.10.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "supertest": "^6.3.3",
    "@types/jest": "^29.5.3",
    "@types/supertest": "^2.0.12"
